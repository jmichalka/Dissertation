	Isbn = {0031-9007},
	Issn = {0031-9007},
	Journal = {Phys. Rev. Lett.},
	Number = {3},
	Pages = {534--537},
	Pmid = {10062835},
	Title = {Coadsorbate Induced Reconstruction of a Stepped \ce{Pt}(111) Surface by Sulfur and \ce{CO}: A Novel Surface Restructuring Mechanism Observed by Scanning Tunneling Microscopy},
