	Isbn = {0039-6028},
	Journal = {Surf. Sci.},
	Month = {9},
	Number = {2},
	Pages = {323--342},
	Title = {Interactions between oxygen and carbon monoxide on a Pd(111) surface},
	Ty = {JOUR},
