	Eprint = {http://www.sciencemag.org/content/315/5809/220.full.pdf},
	Journal = {Science},
	Number = {5809},
	Pages = {220-222},
	Title = {Stabilization of Platinum Oxygen-Reduction Electrocatalysts Using Gold Clusters},
